#include <iostream> 
#include <cstdlib>  
using namespace std; 
// Global variable  
int b = 50; 
// Namespace declarations 
namespace N1 
{ 
int a = 80; 
} 
namespace N2 
{ 
int a = 2; 
} 
// Function to demonstrate local, global, and namespace scope  
void fun() 
{ 
int a = 2; // Local variable inside function  
 
cout << "Fun A : " << a << " " << ::b << endl; // local a, global b 
 
} 
     
int main() 
 
{ 
 
system(“color F0”); 
 
cout << "Roll No: 25B21AI020" << endl; 
 
{int a = 2; // Inner block variable  
cout << "Inner A : " << a << endl; 
} 
 
cout << "Outer B : " << b << endl; 
 
fun(); // Call to function fun() 
 
// Using fully qualified names 
 
cout << "Name space 1 : " << N1::a << endl; 
cout << "Name space 2 : " << N2::a << endl; 
return 0; 
 
}
